<!--ユーザーの詳細-->
<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100, 100) %>
</div>

<!--ユーザーの投稿一覧-->
<% post_images.each do |post_image| %>
  <div>
    <%= link_to post_image_path(post_image.id) do %>
      <%= image_tag post_image.get_image %>
    <% end %>
    <p>投稿ユーザー画像：</p>
    <p>ショップ名：</p>
    <p>説明：</p>
    <p>ユーザーネーム：</p>
  </div>